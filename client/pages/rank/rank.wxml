<!--index.wxml-->
<view class="container" wx:if="{{isshow}}">
   <view class="title">{{title}}</view>
   <view class="item" wx:for="{{pagedata}}">
      <label class="ranknum">{{index+1}}</label><label class="username">{{item.name}}</label><label class="dw">{{item.score}}</label>
   </view>
   <view class='footer'><button bindtap='gotoIndex'>首页</button></view>
</view>
